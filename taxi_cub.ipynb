    "import gym\n",
    "import time\n",
    "env = gym.make('Taxi-v3',render_mode='human')\n",
    "observation = env.reset()\n",
    "count = 0\n",
    "reward = 0\n",
    "while reward != 20:\n",
    "    action = env.action_space.sample()\n",
    "    observation, reward, done, info, extra_value = env.step(action)\n",
    "    count += 1\n",
    "    print(action)"
   ]
